#!/bin/python3

import math
import os
import random
import re
import sys

#####################################################
# Complete the 'plusMinus' function below.
#
# The function accepts INTEGER_ARRAY arr as parameter.
#####################################################

# Start of the code

def plusMinus(arr):
    # Write your code here
    p,q,r=0,0,0
    for a in range(0,len(arr)):
        if arr[a]>0:
            p += 1
        elif arr[a]<0:
            r += 1
        else:
            q += 1
    print(p/len(arr))
    print(r/len(arr))
    print(q/len(arr))

if __name__ == '__main__':
    n = int(input().strip())

    arr = list(map(int, input().rstrip().split()))

    plusMinus(arr)
