#!/bin/python3

import math
import os
import random
import re
import sys

# Complete the miniMaxSum function below.
def miniMaxSum(arr):
    arr.sort()
    keep = [None]*int(len(arr)-3)
    for a in range(0,len(arr)-3):
        s = 0
        for b in range(a,a+4):
            s += arr[b]
        keep[a] = s
    
    print(keep[0],keep[-1])


if __name__ == '__main__':
    arr = list(map(int, input().rstrip().split()))

    miniMaxSum(arr)
